generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model user {
  id         Int      @id @default(autoincrement())
  username   String
  fullName   String
  email      String   @unique
  password   String
  created_at DateTime @default(now())
}
